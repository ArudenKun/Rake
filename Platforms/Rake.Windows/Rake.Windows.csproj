<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net9.0-windows</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <BuiltInComInteropSupport>true</BuiltInComInteropSupport>
    </PropertyGroup>

    <PropertyGroup>
        <ApplicationManifest>app.manifest</ApplicationManifest>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Avalonia.Skia" Version="11.2.3"/>
        <PackageReference Include="Avalonia.Win32" Version="11.2.3"/>
        <PackageReference Include="Microsoft.Windows.CsWin32" Version="0.3.162">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="WebView.Avalonia.Windows.Cross" Version="11.2.3"/>
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\..\Rake\Rake.csproj"/>
    </ItemGroup>

</Project>
